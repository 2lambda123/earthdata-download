{
  "name": "earthdata-download",
  "version": "1.0.0",
  "description": "initial readme",
  "main": "src/main/main.js",
  "scripts": {
    "docs": "./node_modules/.bin/jsdoc -p -c ./.jsdoc.conf.json",
    "postinstall": "npm run docs",
    "start": "electron .",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .jsx,.js",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:watch-lite": "jest --watchAll --collectCoverageFrom='' --coverage=true",
    "test:bamboo": "jest --ci --reporters=default --reporters=jest-junit",
    "preplaywright:coverage": "rm -rf playwright-coverage/ .nyc_output/",
    "playwright": "NODE_ENV=playwright nyc playwright test",
    "playwright:coverage": "PLAYWRIGHT_JUNIT_OUTPUT_NAME=results.xml NODE_ENV=playwright nyc playwright test --reporter=junit",
    "playwright:bamboo": "start-server-and-test 'npm run dev' http://localhost:5173 'xvfb-run --auto-servernum --server-args=\"-screen 0 1920x1080x24\" -- npm run playwright:coverage'",
    "playwright:ui": "npx playwright test --ui",
    "postplaywright:coverage": "npx nyc report --reporter=json --reporter=lcov --reporter=clover --report-dir=playwright-coverage"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@git.earthdata.nasa.gov:7999/edsc/earthdata-download.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/eslint-parser": "^7.21.3",
    "eslint-config-airbnb": "^19.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@playwright/test": "^1.32.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "babel-plugin-istanbul": "^6.1.1",
    "docdash": "^2.0.1",
    "electron": "^24.1.2",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-junit": "^16.0.0",
    "jsdoc": "^4.0.2",
    "nyc": "^15.1.0",
    "playwright": "^1.32.3",
    "playwright-test-coverage": "^1.2.12",
    "start-server-and-test": "^2.0.0",
    "vite": "^4.2.0"
  },
  "nyc": {
    "include": [
      "srcjs/**/"
    ]
  }
}
